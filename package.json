{
  "name": "chatus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/generate-sw.js && node scripts/generate-pwa-icons.js && next dev",
    "build": "node scripts/generate-sw.js && node scripts/generate-pwa-icons.js && next build",
    "build:unsafe": "node scripts/generate-sw.js && node scripts/generate-pwa-icons.js && next build || true",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "preview": "next build && next start",
    "test:emulator": "node tests/emulator/rtdb-emulator-test.mjs",
    "test:unit": "vitest run --reporter=verbose",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:firebase": "vitest run tests/**/*.firebase.test.ts || echo 'Firebase smoke tests completed'",
    "test:clean": "vitest run --no-coverage --reporter=basic",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "perf:check": "npm run type-check && npm run lint && npm run test:unit",
    "perf:build": "npm run build && npm run analyze",
    "perf:analyze": "node scripts/performance-check.js",
    "quality:check": "npm run type-check && npm run lint && npm run test:clean",
    "pwa:icons": "node scripts/generate-pwa-icons.js",
    "pwa:build": "npm run pwa:icons && npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
